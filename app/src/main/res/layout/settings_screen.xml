<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/settingspln">

    <TextView
        android:id="@+id/btn_BACK"
        android:layout_width="96dp"
        android:layout_height="44dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="668dp"
        android:fontFamily="@font/bowlby_one_sc"
        android:text="BACK"
        android:textColor="#3E2C1E"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_Update"
        android:layout_width="182dp"
        android:layout_height="62dp"
        android:layout_gravity="center"
        android:layout_marginStart="48dp"
        android:layout_marginTop="334dp"
        android:layout_marginEnd="181dp"
        android:layout_marginBottom="335dp"
        android:background="none"
        android:fontFamily="@font/bowlby_one_sc"
        android:text="Update"
        android:textColor="#3E2C1E"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_Restart"
        android:layout_width="315dp"
        android:layout_height="58dp"
        android:layout_gravity="center"
        android:layout_marginStart="41dp"
        android:layout_marginTop="266dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="407dp"
        android:background="none"
        android:fontFamily="@font/bowlby_one_sc"
        android:text="Restart Game"
        android:textColor="#3E2C1E"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.SwitchCompat
        android:id="@+id/switch1"
        android:layout_width="255dp"
        android:layout_height="48dp"
        android:layout_gravity="center"
        android:layout_marginStart="78dp"
        android:layout_marginTop="208dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="475dp"
        android:checked="true"
        android:fontFamily="@font/bowlby_one_sc"
        android:text="Audio"
        android:textColor="#3E2C1E"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/btn_CREDITS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="668dp"
        android:layout_marginEnd="52dp"
        android:fontFamily="@font/bowlby_one_sc"
        android:text="CREDITS"
        android:textColor="#3E2C1E"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="125dp"
        android:layout_marginEnd="316dp"
        android:layout_marginBottom="587dp"
        android:fontFamily="@font/bowlby_one_sc"
        android:text="SETTINGS"
        android:textColor="#FF3E2C1E"
        android:textSize="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.371" />
</androidx.constraintlayout.widget.ConstraintLayout>